{
  "name": "vote",
  "repository": "https://github.com/itxchy/FCC-vote",
  "version": "1.0.0",
  "description": "Create and share polls. Vote in polls made by friends and strangers.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js | ./node_modules/.bin/bunyan",
    "test": "NODE_ENV=test jest",
    "tdd": "NODE_ENV=test jest --watch",
    "coverage": "NODE_ENV=test jest --coverage",
    "lint": "eslint 'server.js' webpack.config.js 'server/**' 'routes/**' 'auth/**'; exit 0",
    "build": "webpack",
    "build:prod": "npm run babel:prod && webpack --config webpack.prod.js",
    "babel:clear": "del public/production",
    "babel:components": "NODE_ENV=server babel components -d public/production/components",
    "babel:redux": "NODE_ENV=server babel redux -d public/production/redux",
    "babel:auth": " NODE_ENV=server babel auth -d public/production/auth",
    "copy:css": "mkdir public/production/css && cp -r css/* public/production/css",
    "babel:routes": "NODE_ENV=server babel server/routes -d public/production/server/routes",
    "babel:prod": "npm run babel:clear && mkdir public/production && npm run copy:css && npm run babel:routes && npm run babel:components && npm run babel:redux && npm run babel:auth",
    "watch": "webpack --watch",
    "watch:prod": "webpack --config webpack.prod.js --watch",
    "server": "NODE_ENV=server nodemon server.js | bunyan",
    "mongo": "mongod --port 27017 --dbpath=./data",
    "all": "xfce4-terminal -H -e \"npm run tdd\" & xfce4-terminal -H -e \"npm run server\" & xfce4-terminal -H -e \"npm run watch\" & xfce4-terminal -H -e \"mongod --port 27017 --dbpath=./data\""
  },
  "author": "Matt Trifilo",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "browser-sync": "^2.17.5",
    "browser-sync-webpack-plugin": "^1.1.3",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "del-cli": "^0.2.1",
    "enzyme": "^2.5.1",
    "eslint": "^3.8.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-config-standard-jsx": "^3.2.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.9.0",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "react-addons-test-utils": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "redux-mock-store": "^1.2.2",
    "resolve-url-loader": "^1.6.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.0",
    "webpack-merge": "^3.0.0"
  },
  "dependencies": {
    "asyncawait": "^1.0.6",
    "axios": "^0.15.2",
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-register": "^6.23.0",
    "bcrypt": "^0.8.7",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "bunyan": "^1.8.8",
    "classnames": "^2.2.5",
    "d3": "^4.4.0",
    "express": "^4.14.0",
    "express-static-gzip": "^0.2.2",
    "fast-async": "^6.1.2",
    "font-awesome": "^4.7.0",
    "graphql": "^0.9.1",
    "graphql-subscriptions": "^0.2.1",
    "helmet": "^3.4.1",
    "jquery": "^3.1.1",
    "jsdom": "^9.9.1",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.17.2",
    "module-alias": "^2.0.0",
    "mongoose": "^4.7.2",
    "preact": "^7.2.0",
    "preact-compat": "^3.13.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-faux-dom": "^3.0.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "shortid": "^2.2.6",
    "validator": "^6.1.0"
  }
}
